<h1>{{title}}</h1>
<div class="graph">
    <div class="left half">View</div>
    <div class="right half">
        <div class="premises">
            {{#each argument.premises:i}}
            <Viewer index='{{i+1}}' title={{argument.premiseTitles[i]||false}}>{{argument.premises[i]}}</Viewer>
            {{/each}}
        </div>
    </div>
</div>